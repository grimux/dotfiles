#!/bin/bash

# Startup
#------------------------------------------------------------------------
cd ~
#~/.fehbg				# set background
xcompmgr &				# start xcompmgr for transparency
imwheel -b 45 &				# Faster scolling
#variety &				# Wallpaper
nitrogen --restore			# Wallpaper
#~/.local/bin/dwmbar >/dev/null 2>&1 &	# start dwmbar script to control status bar
conky -c $HOME/.config/conky/conkyrc &	# conky
dwmblocks &				# start dwmblocks status bar
dwm	 				# start dwm
#------------------------------------------------------------------------
