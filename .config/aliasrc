#!/usr/bin/env bash

alias \
	ka="killall" \
	ls="ls -l --color=auto --group-directories-first" \
	la="ls -lA --color=auto --group-directories-first" \
	f="ranger" \
	sf="sudo ranger" \
	e="explorer.exe ." \
	x="exit" \
	t="tmux -f ~/.config/tmux/tmux.conf" \
	c="calcurse -D ~/.config/calcurse" \
	vim="nvim" \
	yt="youtube-dl" \
	ytm="youtube-dl --config-location ~/.config/youtube-dl/music" \
	ytp="youtube-dl --config-location ~/.config/youtube-dl/playlist" \
	wiki="nvim -c VimwikiIndex" \
	y="yadm $*" \
	sdn="shutdown now" \
